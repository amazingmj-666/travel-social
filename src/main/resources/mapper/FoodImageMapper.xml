<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wmj.design.travelsocial.dao.FoodImageMapper">
    <select id="selectFoodImageByFoodId" parameterType="int"
            resultType="com.wmj.design.travelsocial.domain.information.FoodImage">
        SELECT * FROM food_image WHERE food_id=#{foodId}
    </select>
</mapper>
